{
  "name": "quint-demo",
  "version": "1.0.0",
  "description": "A Hugo + Collected Notes static website template.",
  "scripts": {
    "start": "hugo server --disableFastRender",
    "prebuild": "npm i && git submodule update --init --recursive && node utils/fetch-posts.js",
    "build": "hugo --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victoriadrake/quint-demo.git"
  },
  "keywords": [
    "static",
    "site",
    "hugo",
    "collected",
    "notes",
    "cms"
  ],
  "author": "Victoria Drake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victoriadrake/quint-demo/issues"
  },
  "homepage": "https://github.com/victoriadrake/quint-demo#readme",
  "devDependencies": {
    "js-yaml": "^4.1.0"
  },
  "dependencies": {
    "decap-cms-app": "^3.4.0"
  }
}
